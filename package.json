{
  "name": "phekb-export",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn run viewer",
    "viewer": "concurrently --names \"API,UI\" \"nodemon --watch viewer/api -r ts-node/register viewer/api/index.ts\" \"parcel viewer/ui/index.html\"",
    "update-data": "rimraf ./data && yarn start",
    "prod": "concurrently --names \"API,UI\" \"nodemon -r ts-node/register viewer/api/index.ts\" \"parcel serve viewer/ui/index.html\""
  },
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.19.1",
    "@blueprintjs/table": "^3.8.1",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@types/webpack-env": "^1.14.1",
    "babel-polyfill": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.5.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "fetch-cookie": "^0.7.2",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.19.3",
    "normalize.css": "^8.0.1",
    "parcel": "^1.12.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-split": "^2.0.7",
    "slugify": "^1.3.4",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "rimraf": "^2.6.3",
    "sass": "^1.23.0",
    "typescript": "^3.6.4"
  }
}
